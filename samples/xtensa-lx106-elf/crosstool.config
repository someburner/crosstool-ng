CT_CONFIG_VERSION="3"
CT_OBSOLETE=y
CT_EXPERIMENTAL=y
# CT_PREFIX_DIR="${CT_TOP_DIR}/builds/${CT_TARGET}"
CT_PATCH_BUNDLED_LOCAL=y
CT_LOCAL_PATCH_DIR="${CT_TOP_DIR}/local-patches"

### Misc ###
# CT_INSTALL_LICENSES is not set
CT_STRIP_TARGET_TOOLCHAIN_EXECUTABLES=y

### Target ###
CT_TARGET_CFLAGS="-mlongcalls"
CT_TARGET_LDFLAGS="-Wl,--size-opt"

### Arch ###
CT_ARCH="xtensa"
CT_ARCH_XTENSA=y
CT_ARCH_CHOICE_KSYM="XTENSA"
# CT_ARCH_USE_MMU is not set
# CT_CC_GCC_USE_LTO is not set
# CT_CC_GCC_ENABLE_DEFAULT_PIE is not set

CT_XTENSA_CUSTOM=y
CT_ARCH_XTENSA_CUSTOM_NAME="lx106"
CT_ARCH_XTENSA_CUSTOM_OVERLAY_LOCATION="${CT_TOP_DIR}/overlays"

CT_OVERLAY_NAME="lx106"
CT_OVERLAY_LOCATION="${CT_TOP_DIR}/overlays"
CT_TARGET_VENDOR="lx106"

### NEWLIB / LIBC ###
CT_NEWLIB_V_2_2=y
# set to develop src, no patches
CT_NEWLIB_SRC_DEVEL=y
CT_NEWLIB_DEVEL_VCS_git=y
CT_NEWLIB_DEVEL_VCS="git"
CT_NEWLIB_DEVEL_URL="https://github.com/someburner/newlib-xtensa.git"
CT_NEWLIB_DEVEL_BRANCH="xtensa-2_2_0-opensdk"
CT_NEWLIB_DEVEL_REVISION=""
CT_NEWLIB_DEVEL_SUBDIR=""
CT_NEWLIB_DEVEL_BOOTSTRAP=""
CT_NEWLIB_PATCH_NONE=y
CT_NEWLIB_PATCH_ORDER="none"
# flags
CT_LIBC_NEWLIB_TARGET_CFLAGS="-DMALLOC_PROVIDED -ffunction-sections -fdata-sections -Wl,--size-opt"
CT_LIBC_NEWLIB_DISABLE_SUPPLIED_SYSCALLS=y
CT_LIBC_NEWLIB_NANO_FORMATTED_IO=y
# CT_LIBC_NEWLIB_FVWRITE_IN_STREAMIO is not set
# CT_LIBC_NEWLIB_UNBUF_STREAM_OPT is not set
# CT_LIBC_NEWLIB_FSEEK_OPTIMIZATION is not set
# CT_LIBC_NEWLIB_ATEXIT_DYNAMIC_ALLOC is not set
CT_LIBC_NEWLIB_LITE_EXIT=y
CT_LIBC_NEWLIB_REENT_SMALL=y
CT_LIBC_NEWLIB_MULTITHREAD=y
CT_LIBC_NEWLIB_EXTRA_SECTIONS=y
# CT_LIBC_NEWLIB_WIDE_ORIENT is not set

### GCC ###
CT_CC_LANG_CXX=y
CT_CC_GCC_ENABLE_CXX_FLAGS="-fno-exceptions -fno-rtti"
# CT_CC_CXA_ATEXIT is not set
# CT_CC_GCC_STATIC_LIBSTDCXX is not set
CT_CC_GCC_LDBL_128=y
# CT_CC_GCC_CONFIG_TLS is not set
# CT_CC_GCC_USE_GRAPHITE is not set

### ISL ###
# CT_COMP_LIBS_ISL is not set
# CT_ISL_NEEDED is not set
# CT_ISL is not set
